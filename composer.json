{
    "name": "buzzingpixel/corbomite-migrations",
    "version": "1.0.0",
    "description": "Corbomite migrations",
    "authors": [
        {
            "name": "TJ Draper",
            "email": "tj@buzzingpixel.com",
            "homepage": "https://buzzingpixel.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "buzzingpixel/corbomite-di": "^1.0",
        "buzzingpixel/corbomite-cli": "^1.0.3",
        "robmorgan/phinx": "^0.10.6"
    },
    "require-dev": {
        "roave/security-advisories": "dev-master",
        "squizlabs/php_codesniffer": "^3.4",
        "phpmd/phpmd": "^2.6",
        "vlucas/phpdotenv": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "corbomite\\migrations\\": "src"
        }
    },
    "extra": {
        "diConfigFilePath": "src/diConfig.php",
        "cliActionConfigFilePath": "src/actionConfig.php"
    }
}
